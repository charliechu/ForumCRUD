<%= render :partial => "common/login_info" %>

<h1 style="border:1px solid khaki ; background: oldlace;">編輯類別名稱</h1>

<%= form_for @category, :url => category_path(@category), :html => {:method => :put}  do |f| %>
    <%= f.label :name, "名稱" %>
    <%= f.text_field :name %>
    
    <%= f.submit "更新" %>
<% end %>
